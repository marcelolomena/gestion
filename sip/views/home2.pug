                  -var days = ["Dom","Lun","Mar","Mier","Jue","Vie","Sab"];
                  -var meses = ["Ene", "Feb", "Mar", "Abr", "May", "Jun", "Jul", "Ago", "Sep", "Oct", "Nov", "Dic"];
                  -var fecha = new Date();
                  -var inicioSemana = fecha.getDate() - fecha.getDay() + (fecha.getDay() === 0 ? -6 : 1);
                  -var finSemana = fecha.getDate() - fecha.getDay() + (fecha.getDay() === 6 ?  1: 7);
                  -var primerDia = new Date(fecha.getFullYear(), fecha.getMonth(), 1);
                  -var ultimoDia = new Date(fecha.getFullYear(), fecha.getMonth() + 1, 0);
                  span.text-muted #{(days[fecha.getDay()])} #{(fecha.getDate())}
                 a.list-group-item.justify-content-between(href='#')
                  span Ayer
                  span.text-muted #{(days[fecha.getDay()-1])} #{(fecha.getDate()-1)}
