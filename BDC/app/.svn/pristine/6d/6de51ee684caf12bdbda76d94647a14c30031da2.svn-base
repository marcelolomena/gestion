								               [0.6, '#555']
								            ]
								         },
								         stroke: '#000000',
								         style: {
								            color: '#CCC',
								            fontWeight: 'bold'
								         },
								         states: {
								            hover: {
								               fill: {
								                  linearGradient: { x1: 0, y1: 0, x2: 0, y2: 1 },
								                  stops: [
								                     [0.4, '#BBB'],
								                     [0.6, '#888']
								                  ]
								               },
								               stroke: '#000000',
								               style: {
								                  color: 'white'
								               }
								            },
								            select: {
								               fill: {
								                  linearGradient: { x1: 0, y1: 0, x2: 0, y2: 1 },
								                  stops: [
								                     [0.1, '#000'],
								                     [0.3, '#333']
								                  ]
								               },
								               stroke: '#000000',
								               style: {
								                  color: 'yellow'
								               }
								            }
								         }
								      },
								      inputStyle: {
								         backgroundColor: '#333',
								         color: 'silver'
