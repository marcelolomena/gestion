@(delayLevelValues: java.util.HashMap[String, String], projectClassificationValues : java.util.HashMap[String, String],modelManagementValues : java.util.HashMap[String, String], divisionValues : java.util.HashMap[String, String], programSubTypeValues : java.util.HashMap[String, String], budgetTypeValues: java.util.HashMap[String, String], programManagerValues : java.util.HashMap[String, String], sortValues : java.util.HashMap[String, String],searchProgram : Form[ProgramSearch])(implicit session: play.api.mvc.Session,lang: Lang)
@import play.api.i18n._
@import play.api._
@import services._
@import helper._
<div class="focus_on width_full" style="padding:10px 0px; margin:20px 0px;" id = "search_program_id">
	 	<div class="focus_on_title"><img width="94" height="13" alt="" src="@routes.Assets.at("frontend/images/modify_search.png")"></div>
	 	<div class="search-form-wrapper" style="padding: 10px 40px;">
		@form(action = controllers.Frontend.routes.Program.searchResult, 'id->"search-program-form") {
		 	<div class="field-wrapper">
	        	@select(
            		searchProgram("delay_level"),
       				options(delayLevelValues),
					'class -> "input-xlarge ",
					'_label -> Messages("programsearch.delaylevel"),
					'_default -> Messages("programsearch.selectdelaylevel")
				)
            </div>
            <div class="field-wrapper"> 
            	 @select(
		            	searchProgram("project_classification"),
		       			options(projectClassificationValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.projectclassification"),
						'_default -> Messages("programsearch.selectprojectclassification"))
            </div>
            
            <div class="field-wrapper" > 
            	@select(
		            	searchProgram("program_type"),
		       			options(modelManagementValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.programtype"),
						'_default -> Messages("programsearch.selectprogramtype"))
            </div>
            
            <div class="field-wrapper"> 
            	@select(
		            	searchProgram("program_sub_type"),
		       			options(programSubTypeValues),
						'class -> "input-xlarge ", 
						'_label -> Messages("programsearch.programsubtype"),
						'_default -> Messages("programsearch.selectprogramsubtype")) 
						
            </div>
        
            <div class="field-wrapper">
              @select(
		            	searchProgram("division"),
		       			options(divisionValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.division"),
						'_default -> Messages("programsearch.selectdivision"))
              
            	
            </div>
            
            <div class="field-wrapper "> 
              		
              		@select(
		            	searchProgram("program_role"),
		       			options(programManagerValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.programrole"),
						'_default -> Messages("programsearch.selectprogramrole"))
              
            	@*<dl id="tasks_field" class=" ">
				    <dt><label for="program_role">Program Managers</label></dt>
				    <dd>
					    <select class="input-xlarge " name="program_role" id="program_role">
					            <option value="" class="blank">--- Choose Roles ---</option>
					           
					    </select>
					</dd>
				</dl>*@
            </div>
            
            <div style="width:100%; float:left">
	            <div class="field-wrapper" > 
	            		@select(
		            	searchProgram("item_budget"),
		       			options(budgetTypeValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.budgettype"),
						'_default -> Messages("programsearch.selectbudgettype"))
	            	
	            </div>
	            
	            <div class="field-wrapper" > 
	            		@select(
		            	searchProgram("sort_field"),
		       			options(sortValues),
						'class -> "input-xlarge ",
						'_label -> Messages("programsearch.sorttype"),
						'_default -> Messages("programsearch.selectsorttype"))
	            	
	            </div>
            </div>
            
            <div class="" style="margin: 10px auto;text-align: center;width: 300px;">
            	<span class="left" style="margin-top: 15px;"><input class="btn form-button" id="program-search-form" value='@Messages("button.search")' type="button"/></span>
    						<span class="left" style="margin-left: 29px; margin-top: 15px;"> <input id="cancel-program-search-report" class="btn form-button" value='@Messages("button.cancel")' type="button"/></span>
            </div>
        }	
        </div>
</div>
