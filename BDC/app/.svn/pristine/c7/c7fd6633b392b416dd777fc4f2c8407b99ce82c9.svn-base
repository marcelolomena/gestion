@(delayLevelValues: java.util.HashMap[String, String], projectClassificationValues : java.util.HashMap[String, String], modelManagementValues : java.util.HashMap[String, String], divisionValues : java.util.HashMap[String, String], programSubTypeValues : java.util.HashMap[String, String], searchDashboardForm : Form[DashboardSearch])(implicit session: play.api.mvc.Session,lang: Lang,req : play.api.mvc.RequestHeader)
@frontend.layout()
@import play.api.i18n._
@import play.api._
@import services._
@import helper._

<style type="text/css">
#chartdiv {
	width		: 100%;
	height		: 500px;
	font-size	: 11px;
}
</style>


</head>
<body>
@frontend.topLayout()
<div class="main" style="width: 1020px;">
@frontend.header()
<!-- <section class="slider">
    <div class="map"><img src="@routes.Assets.at("frontend/images/dash.jpg")" width="960" alt=""></div>
</section> -->
<section class="slider" style="width:auto; min-width: 936px;">
    <div class="dash_tabbing" style="width:auto; min-width: 936px;">
      <div class="content-box" style="width:auto; min-width: 936px;">
