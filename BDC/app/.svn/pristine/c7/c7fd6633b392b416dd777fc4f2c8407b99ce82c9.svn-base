          <input type="hidden" name="riskstatus" id="riskLabelstatus" value="@Messages("dashboard.risk")" />	
          <ul class="content-box-tabs dashboard-overview-tab" style="width:100%;">
            @*<li><a class="tab1" href="javascript:void(0)">@Messages("dashboard.financials") </a></li>
              <li><a href="javascript:void(0)" class="tab2">@Messages("dashboard.employees") </a></li>
           @* <li><a href="javascript:void(0)" class="tab3">PROJECTS</a></li>*@
           @* <li><a href="javascript:void(0)" class="tab4">@Messages("dashboard.timesheet")</a></li>*@
           
              <li><a href="javascript:void(0)" id="dashboard-tab1" class="dashboard-tab1 current">@Messages("dashboard.report")</a></li>
                <li><a href="javascript:void(0)" id="dashboard-risk-tab" class="">Risk Report</a></li>
                <li><a href="javascript:void(0)" id="dashboard-issue-tab" class="">Issue Report</a></li>
              @*<li><a href="javascript:void(0)" class="tab_new_pi">@Messages("dashboard.KEYIndicator")</a></li>*@
              
              <li  style="float:right;margin:0px;"><a href="javascript:void(0)" id="dashboard-tab2" class="dashboard-tab2">@Messages("dashboard.searchdashboard") </a></li>
          </ul>
           @*<a style="color: #315663;font-weight: bold;float: right; margin-right: 10px; margin-top: 25px;" href="/report-tool">@Messages("dashboard.reportlink") </a>*@ 
        </div>
        <div class="content-box-content">
         
        </div>
        
      </div>
    </div>
	@frontend.footer()
	<div class="loader"><img src="@routes.Assets.at("frontend/images/loading.gif")" /></div>
</div>
  </section>
</div>
@frontend.headerScripts()
<link rel="stylesheet" href="@routes.Assets.at("frontend/stylesheets/bar-chart.css")">
<script src="@routes.Assets.at("frontend/js/main.js")" type="text/javascript"></script>
<script src="@routes.Assets.at("frontend/js/excellentexport.js")" type="text/javascript"></script>
<script src="@routes.Assets.at("frontend/js/jquery.jqplot.min.js")" type="text/javascript"></script>
<script src="@routes.Assets.at("frontend/js/jqplot.barRenderer.min.js")" type="text/javascript"></script>
