<script src="@routes.Assets.at("frontend/js/jqplot.pointLabels.min.js")" type="text/javascript"></script>
<script src="@routes.Assets.at("frontend/js/jqplot.pieRenderer.min.js")" type="text/javascript"></script>
<script src="@routes.Assets.at("frontend/js/jqplot.donutRenderer.min.js")" type="text/javascript"></script>
<link rel="stylesheet" href="@routes.Assets.at("frontend/stylesheets/documents.css")">

